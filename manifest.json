{
  "manifest_version": 3,
  "name": "Medusa - Ask to AI v1.0.4",
  "version": "1.0.4",
  "description": "Medusa - Ask to AI: ChatGPT, Claude, DeepSeek, Copilot, Gemini, Grok, Meta, Mistral, Google, Perplexity or ALL AI",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs",
    "contextMenus"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle",
      "all_frames": false
    }
  ]
} 